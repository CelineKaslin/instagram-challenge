<h1> Post Pic </h1>
<%= form_for @pic do |form| %>
  <%= form.label :title %>
  <%= form.text_field :title %><br><br>
  <%= form.label :description %>
  <%= form.text_area :description, :cols =>30, :rows => 8 %><br><br>

  <%= form.submit "Submit" %>
<% end %><br>

<%= link_to root_path do %>
  Back to Home Page
<% end %>
